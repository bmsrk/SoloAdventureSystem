{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "AI": {
    "Provider": "LLamaSharp",
    "Model": "phi-3-mini-q4",
    "LLamaModelKey": "phi-3-mini-q4",
    "ContextSize": 4096,
    "UseGPU": true,
    "MaxInferenceThreads": 4,
    "Temperature": 0.7,
    "EnableCaching": true,
    "CacheDirectory": ".aicache",
    "MaxRetries": 3,
    "TimeoutSeconds": 120
  }
}
